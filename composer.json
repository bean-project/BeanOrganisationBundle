{
  "name": "bean-project/organisation-bundle",
  "type": "symfony-bundle",
  "description": "Symfony BeanOrganisationBundle",
  "keywords": ["organisation manager","company management bundle", "bundle", "bean"],
  "license": "MIT",
  "authors": [
    {
      "name": "Peter Pierre",
      "email": "peter@bean-project.org"
    }
  ],
  "require": {
    "symfony/framework-bundle": "^3.1",
    "bean-project/core-bundle": "dev-master"
