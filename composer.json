  "conflict": {
    "jms/serializer": "<0.13",
    "sonata-project/seo-bundle": "<1.1.5",
    "sonata-project/block-bundle": "<2.2.8"
  },
  "minimum-stability": "dev",
  "prefer-stable": true,
  "autoload": {
    "psr-4": { "Bean\\Bundle\\OrganisationBundle\\": "" }
  },
  "extra": {
    "branch-alias": {
      "dev-master": "0.1.x-dev"
    }
  }
